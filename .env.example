# Vizwright Environment Variables
# Copy this file to .env and customize as needed

# Port Configuration
VIZWRIGHT_SERVER_PORT=3000
VIZWRIGHT_UI_PORT=5173

# Sample App Credentials
# These are used by sample applications for testing
# VITE_ prefix makes them accessible in both Vite apps and backend
VITE_USER_EMAIL=user@example.com
VITE_USER_PWD=user123
VITE_ADMIN_EMAIL=admin@example.com
VITE_ADMIN_PWD=admin123
